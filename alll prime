public class allprime {
   public static boolean isPrime(int  n ){
    if(n==2){
        return true ;

    }
    for(int i =2;i<=Math.sqrt(n);i++){
      if(n%2==0){
        return false;

      }
    }
   
return true ;
}

public static void primeinRange(int n ){
    for(int i=2;i<=n;i++){
        if(isPrime(i)){
            System.out.println(i);
            
        }
    }
    
    
}
    public static void main(String[] args) {
        primeinRange(20);
    }
}
