
import java.util.Scanner;

public class sum {
    public static int sumfuntion(int num1, int num2){

        
        int sum = num2+ num1+5;
        return sum;
    }
    
        
    
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
     int answer = sumfuntion(a,b);
     System.out.println("the sum is :"+ answer);
}
}
