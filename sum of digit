
public class sumofdigit {

     public static void main(String[] args) {
       sumofdigit(19);
       
}
public static void sumofdigit(int n ){
   
   int sum= 0;
   while(n>0){
    int lstdigit = n%10;

    sum = sum + lstdigit;
    n/=10;
   
   }
   System.out.println("the sum is "+ sum);
}

}
